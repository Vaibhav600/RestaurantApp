                            </af:selectOneRadio>
                            <af:inputText value="#{restaurantMenuBean.cuisine}" label="#{bindings.Cuisine.hints.label}"
                                          required="#{bindings.Cuisine.hints.mandatory}"
                                          columns="#{bindings.Cuisine.hints.displayWidth}"
                                          maximumLength="#{bindings.Cuisine.hints.precision}"
                                          shortDesc="#{bindings.Cuisine.hints.tooltip}" id="it11">
                                <f:validator binding="#{bindings.Cuisine.validator}"/>
                            </af:inputText>
                            <f:facet name="footer">
                                <af:button text="Submit" id="b2" action="#{restaurantMenuBean.addMenuItem}"/>
                            </f:facet>
                        </af:panelFormLayout>
                    </af:panelBox>
                    <af:panelBox text="Menu Items" id="pb2">
                        <f:facet name="toolbar"/>
                        <af:listView value="#{bindings.MenuItemsForRestaurantForUserVO.collectionModel}" var="item"
                                     emptyText="#{bindings.MenuItemsForRestaurantForUserVO.viewable ? 'No data to display.' : 'Access Denied.'}"
                                     fetchSize="#{bindings.MenuItemsForRestaurantForUserVO.rangeSize}" id="lv4"
                                     partialTriggers="::b2 b5">
                            <af:listItem id="li4">
                                <af:panelGridLayout id="pgl9">
                                    <af:gridRow marginTop="5px" height="auto" id="gr10">
                                        <af:gridCell marginStart="5px" width="24%" id="gc17">
                                            <af:outputText value="#{item.DishName}"
                                                           shortDesc="#{bindings.MenuItemsForRestaurantForUserVO.hints.DishName.tooltip}"
                                                           id="ot7" inlineStyle="font-size:large; font-weight:bold;"/>
                                        </af:gridCell>
                                        <af:gridCell marginStart="5px" width="33%" id="gc18">
                                            <af:outputText value="#{item.Price}"
                                                           shortDesc="#{bindings.MenuItemsForRestaurantForUserVO.hints.Price.tooltip}"
                                                           id="ot8">
                                                <af:convertNumber groupingUsed="false"
                                                                  pattern="#{bindings.MenuItemsForRestaurantForUserVO.hints.Price.format}"/>
                                            </af:outputText>
                                        </af:gridCell>
                                        <af:gridCell marginStart="5px" width="13%" marginEnd="5px" id="gc19">
                                            <af:outputText value="#{item.ItemId}"
                                                           shortDesc="#{bindings.MenuItemsForRestaurantForUserVO.hints.ItemId.tooltip}"
                                                           id="ot11" rendered="false">
                                                <af:convertNumber groupingUsed="false"
                                                                  pattern="#{bindings.MenuItemsForRestaurantForUserVO.hints.ItemId.format}"/>
                                            </af:outputText>
                                        </af:gridCell>
                                    </af:gridRow>
                                    <af:gridRow marginTop="5px" height="auto" id="gr11">
                                        <af:gridCell marginStart="5px" width="24%" id="gc20">
                                            <af:panelLabelAndMessage label="Availability" id="plam8">
                                                <af:outputText value="#{item.Availability}"
                                                               shortDesc="#{bindings.MenuItemsForRestaurantForUserVO.hints.Availability.tooltip}"
                                                               id="ot20"/>
                                            </af:panelLabelAndMessage>
                                        </af:gridCell>
                                        <af:gridCell marginStart="5px" width="33%" id="gc21"/>
                                        <af:gridCell marginStart="5px" width="13%" marginEnd="5px" id="gc22"/>
                                    </af:gridRow>
                                    <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr12">
                                        <af:gridCell marginStart="5px" width="24%" id="gc23">
                                            <af:panelLabelAndMessage label="Cuisine" id="plam9">
                                                <af:outputText value="#{item.Cuisine}"
                                                               shortDesc="#{bindings.MenuItemsForRestaurantForUserVO.hints.Cuisine.tooltip}"
                                                               id="ot43"/>
                                            </af:panelLabelAndMessage>
                                        </af:gridCell>
                                        <af:gridCell marginStart="5px" width="33%" id="gc24">
