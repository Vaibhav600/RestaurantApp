                                                 fetchSize="#{bindings.BestSellerForRestaurantForUserVO.rangeSize}"
                                                 id="lv6">
                                        <af:listItem id="li6">
                                            <af:panelGridLayout id="pgl14">
                                                <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr19">
                                                    <af:gridCell marginStart="5px" width="50%" id="gc40">
                                                        <af:panelLabelAndMessage label="Item" id="plam16">
                                                            <af:outputText value="#{item.DishName}"
                                                                           shortDesc="#{bindings.BestSellerForRestaurantForUserVO.hints.DishName.tooltip}"
                                                                           id="ot51"/>
                                                        </af:panelLabelAndMessage>
                                                    </af:gridCell>
                                                    <af:gridCell marginStart="5px" width="50%" marginEnd="5px"
                                                                 id="gc41">
                                                        <af:panelLabelAndMessage label="Order" id="plam17">
                                                            <af:outputText value="#{item.TotalOrders}"
