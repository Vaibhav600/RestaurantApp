                                                        <af:panelLabelAndMessage label="Reserved Seats" id="plam22">
                                                            <af:outputText value="#{item.TotalReservedSeats}"
                                                                           shortDesc="#{bindings.TotalReservedSeatsOfRestaurantForUserVO1.hints.TotalReservedSeats.tooltip}"
                                                                           id="ot63">
                                                                <af:convertNumber groupingUsed="false"
                                                                                  pattern="#{bindings.TotalReservedSeatsOfRestaurantForUserVO1.hints.TotalReservedSeats.format}"/>
                                                            </af:outputText>
                                                        </af:panelLabelAndMessage>
                                                    </af:gridCell>
                                                </af:gridRow>
                                                <af:gridRow marginTop="5px" height="40px" id="gr33">
                                                    <af:gridCell marginStart="5px" width="50%" marginEnd="5px"
                                                                 id="gc61">
                                                        <af:panelLabelAndMessage label="Available Seats" id="plam23">
                                                            <af:outputText value="#{item.bindings.AvailableSeats.inputValue}"
                                                                           id="ot64">
                                                                <af:convertNumber groupingUsed="false"
                                                                                  pattern="#{bindings.TotalReservedSeatsOfRestaurantForUserVO1.hints.AvailableSeats.format}"/>
                                                            </af:outputText>
                                                        </af:panelLabelAndMessage>
                                                    </af:gridCell>
                                                </af:gridRow>
                                                <af:gridRow marginTop="5px" height="40px" marginBottom="5px" id="gr34">
                                                    <af:gridCell marginStart="5px" width="50%" marginEnd="5px"
                                                                 id="gc62">
                                                        <af:panelLabelAndMessage label="Reservation Time" id="plam24">
                                                            <af:outputText value="#{item.ReservationTime}"
                                                                           shortDesc="#{bindings.TotalReservedSeatsOfRestaurantForUserVO1.hints.ReservationTime.tooltip}"
