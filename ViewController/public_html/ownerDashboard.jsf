                                        <dvt:pieDataItem id="di1" label="#{row.OrderStatus}" value="#{row.OrderCount}"/>
                                    </dvt:pieChart>
                                </af:panelBox>
                            </af:gridCell>
                            <af:gridCell id="gc39" halign="stretch">
                                <af:panelBox text="Best Seller" id="pb13">
