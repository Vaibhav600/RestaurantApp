                                            <dvt:sparkItem value="#{row.TotalOrders}" id="si3"/>
                                        </af:iterator>
                                    </dvt:sparkChart>
                                </af:panelBox>
                            </af:gridCell>
                        </af:gridRow>
                        <af:gridRow id="gr18">
                            <af:gridCell id="gc38" halign="stretch">
                                <af:panelBox text="Order Status" id="pb14">
                                    <f:facet name="toolbar"/>
