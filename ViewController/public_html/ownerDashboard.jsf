                                                                           id="ot52">
                                                                <af:convertNumber groupingUsed="false"
                                                                                  pattern="#{bindings.BestSellerForRestaurantForUserVO.hints.TotalOrders.format}"/>
                                                            </af:outputText>
                                                        </af:panelLabelAndMessage>
                                                    </af:gridCell>
                                                </af:gridRow>
                                            </af:panelGridLayout>
                                        </af:listItem>
                                    </af:listView>
                                </af:panelBox>
                            </af:gridCell>
                        </af:gridRow>
                        <af:gridRow id="gr20">
                            <af:gridCell id="gc42">
                                <af:panelBox text="Revenue of last 7 days" id="pb15">
                                    <f:facet name="toolbar"/>
                                    <dvt:sparkChart id="sparkchart2" subType="lineWithArea" axisScaledFromBaseline="off"
                                                    areaColor="#99b5ffde" inlineStyle="height:200px;width:200px;">
                                        <af:iterator value="#{bindings.RevenueOfRestaurantVO.collectionModel}" var="row"
                                                     id="i2">
                                            <dvt:sparkItem value="#{row.TotalRevenue}" id="si1"/>
                                        </af:iterator>
                                    </dvt:sparkChart>
                                </af:panelBox>
                            </af:gridCell>
                            <af:gridCell id="gc43"/>
                        </af:gridRow>
                    </af:panelGridLayout>
                </af:showDetailItem>
                <af:showDetailItem id="menu" text="Menu">
                    <af:panelBox text="Add Menu Item" id="pb1">
