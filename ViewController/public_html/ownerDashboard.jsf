                                                                           id="ot56">
                                                                <af:convertNumber groupingUsed="false"
                                                                                  pattern="#{bindings.TotalReservedSeatsOfRestaurantForUserVO1.hints.TotalSeats.format}"/>
                                                            </af:outputText>
                                                        </af:panelLabelAndMessage>
