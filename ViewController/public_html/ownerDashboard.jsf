                                        </af:listItem>
                                    </af:listView>
                                </af:panelBox>
                            </af:gridCell>
                            <af:gridCell marginStart="5px" width="50%" marginEnd="5px" id="gc35">
                                <af:panelBox text="Number of orders placed in last 7 days" id="pb11">
                                    <f:facet name="toolbar"/>
