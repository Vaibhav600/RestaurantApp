  <SQLQuery><![CDATA[SELECT 
    r.restaurant_id, 
    r.name,
    r.owner_id,
    r.available_seats,
    r.phone,
    r.status,
    r.address,
    r.created_at,
    r.updated_at,
    COALESCE(AVG(rr.rating), 0) AS avg_rating
FROM 
    g3_restaurants r
LEFT JOIN 
    g3_orders o ON r.restaurant_id = o.restaurant_id
LEFT JOIN 
    g3_ratings_reviews rr ON o.order_id = rr.order_id
GROUP BY 
    r.restaurant_id, 
    r.name,
    r.owner_id,
    r.available_seats,
    r.phone,
    r.status,
    r.address,
    r.created_at,
    r.updated_at,
    rr.rating
ORDER BY 
    r.restaurant_id, 
    rr.rating]]></SQLQuery>
