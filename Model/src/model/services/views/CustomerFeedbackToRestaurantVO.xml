<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CustomerFeedbackToRestaurantVO"
  Version="12.2.1.22.48"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.services.views.CustomerFeedbackToRestaurantVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT 
    f.feedback_id,
    f.description,
    f.restaurant_id,
    u.full_name,
    f.feedback_date
FROM 
    g3_feedback f
JOIN 
    g3_users u 
ON 
    f.customer_id = u.user_id]]></SQLQuery>
  <ViewAttribute
    Name="FeedbackId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
